<script>let currentText = "";let command = "";document.onkeydown(event);function render(){document.getElementById("main").innerHTML = currentText;}function excCommand(command){}function magicFunc(keycode){//A-Z65-90 1234567890: 48-5738 UP 40 DOWN 32 SPACE 13 Enter 8 BACKSPACElet magicChar = " "switch(keycode){case 48 :magicChar = "0";break;case 49 :magicChar = "1";break;case 50 :magicChar = "2";break;case 51 :magicChar = "3";break;case 52 :magicChar = "4";break;case 53 :magicChar = "5";break;case 54 :magicChar = "6";break;case 55 :magicChar = "7";break;case 56 :magicChar = "8";break;case 57 :magicChar = "9";break;case 65 :magicChar = "a";break;case 66 :magicChar = "b";break;case 67 :magicChar = "c";break;case 68 :magicChar = "d";break;case 69 :magicChar = "e";break;case 70 :magicChar = "f";break;case 71 :magicChar = "g";break;case 72 :magicChar = "h";break;case 73 :magicChar = "i";break;case 74 :magicChar = "j";break;case 75 :magicChar = "k";break;case 76 :magicChar = "l";break;case 77 :magicChar = "m";break;case 78 :magicChar = "n";break;case 79 :magicChar = "o";break;case 80 :magicChar = "p";break;case 81 :magicChar = "q";break;case 82 :magicChar = "r";break;case 83 :magicChar = "s";break;case 84 :magicChar = "t";break;case 85 :magicChar = "u";break;case 86 :magicChar = "v";break;case 87 :magicChar = "w";break;case 88 :magicChar = "x";break;case 89 :magicChar = "y";break;case 32 :magicChar = " ";break;case 13 :excCommand(command);command = "";magicChar = "";break;case 8 :command = command.substring(0,command.length-1);magicChar = "";break;}command += magicChar;currentText += magicChar;}</script>